  },
  "homepage": "https://github.com/zrichardet/Rental-Calc#readme",
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.4.5",
    "@babel/node": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/register": "7.5.5",
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "ghooks": "2.0.4",
    "istanbul": "0.4.5",
    "json-loader": "0.5.7",
    "mocha": "10.2.0",
